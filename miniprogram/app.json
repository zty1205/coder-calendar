{
  "pages": [
    "pages/index/index",
    "pages/user/index",
    "pages/mine/index"
  ],
  "subPackages": [
    {
      "root": "pages_about/",
      "pages": [
        "about/index",
        "version/index"
      ],
      "name": "pages_about"
    },
    {
      "root": "pages_ability/",
      "pages": [
        "food/index"
      ],
      "name": "pages_ability"
    },
    {
      "root": "pages_time/",
      "pages": [
        "calendar/index"
      ],
      "name": "pages_time"
    }
  ],
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": [
        "pages_ability",
        "pages_time"
      ]
    }
  },
  "tabBar": {
    "color": "black",
    "selectedColor": "#6494ed",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home_h.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/mine/index",
        "iconPath": "static/tabbar/mine.png",
        "selectedIconPath": "static/tabbar/mine_h.png",
        "text": "我的"
      }
    ]
  },
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "Weixin",
    "navigationBarTextStyle": "black"
  },
  "lazyCodeLoading": "requiredComponents",
  "sitemapLocation": "sitemap.json",
  "usingComponents": {
    "van-field": "./weapp/field/index",
    "van-cell-group": "./weapp/cell-group/index",
    "van-cell": "./weapp/cell/index",
    "van-popup": "./weapp/popup/index",
    "van-picker": "./weapp/picker/index",
    "van-calendar": "./weapp/calendar/index",
    "van-button": "./weapp/button/index"
  }
}